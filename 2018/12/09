public String decodeString(ArrayList<StringPart> parts){
    String temp="";
    for(int i=0;i<parts.size();i++){
    temp+=masterString.substring(parts.get(i).getStart(),parts.get(i).getStart()+parts.get(i).getLength());
    }
    return temp;
}
public ArrayList<StringPart> encodeString(String word){
    ArrayList<StringPart> temp=new ArrayList<StringPart>();
    while(word.length()!=0){
        StringPart part=findPart.(word);
        temp.add(part);
        word=word.subString(part.getLength());
    }
    return temp;
    
 }
       
    
    
